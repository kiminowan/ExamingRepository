/* pages/practice1/practice1.wxss */
.view_question{
  width: 350px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  min-height: 100px;
}
.select{
  width: 220px;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 65px;
  line-height: 14px;
  margin-top: 20px;
  font-size: 14px;
}
.btn{
  height: 60px;
  width: 140px;
  line-height: 60px;
}
.btn_prev{
  float: left;
  width: 100px;
}
.btn_next{
  float: right;
  width: 100px;
}
.btn_submit{
  float: left;
  margin-left: 28px;
  width: 120px;
}
.check{
  width: 300px;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 60px;
  line-height: 20px;
  margin-top: 10px;
  margin-left: 40px;
  font-size: 14px;
}
.text_answer{
  color: red;
}
.view_answer{
  min-height: 93px;
  margin-top: 10px;
}
.btn_analysis{
  margin-top: 10px;
  width: 140px;
}
.view_currentAnswer{
  text-align: center;
}
.index{
  height: 120px;
  width: 30px;
  position: fixed;
  left: 0;
  top: 50%;
  margin-top: -80px;
}
.index view{
margin: 8px 0 0 -8px;
}
.img1{
  width: 100%;
  height: 100%;
  z-index: -2;
  
}
.view_dui{
  width: 80px;
  height: 80px;
  margin-top: 30px;
  margin-left: 40px;
  float: left;
}
.view_cuo{
  width: 100px;
  height: 80px;
  margin-top: 30px;
  margin-right: 40px;
  float: right;
}
.btn_panduan{
  width: 100px;
  height: 80px;
  margin-top: 20px;
}


.demo-container{
	width:50vw;
	height: 100vh;
	background:#fff;
}